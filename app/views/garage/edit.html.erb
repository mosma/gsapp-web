<div class="row">
  <h1><%= my_garage.name %> <small>profile</small></h1>
  <section class="col-md-2">
<ul class="nav nav-pills nav-stacked">
  <li><%= link_to 'Garage Info', '#'%></li>
  <li><%= link_to 'Localization', '#'%></li>
  <li><%= link_to 'Profile Info', '#'%></li>
  <li><%= link_to 'Statistics', '#'%></li>
</ul>
  </section>
  <section class="col-md-10">
    <div class="thumbnail">
      <style type="text/css" media="screen">
        .thumbnail .img<%= my_garage.id  %> {
          background-image: url(<%= my_garage.avatar  %>);
          height: 60px;
        }
      </style>
      <div class="img img<%= my_garage.id  %>"></div>
    </div>

    <div>
           
<%= form_for(my_garage, url: profile_path) do |f| %> 

<div class="form-group">
  <%= f.label :status %>
  <%= f.select :status, my_garage.available_status, class: "form-control" %>  
</div>

<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :slug %>
  <%= f.text_field :slug, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :link %>
  <%= f.text_field :link, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :tags %>
  <ul id="tagit-tags"></ul>
  <%= f.hidden_field :tags, class: "form-control", id: 'tags-field', value: my_garage.tags.join(',') %>
</div>
<div class="form-group">
  <%= f.label :description %>
  <%= f.text_area :description, class: "form-control" %>  
</div>
 <div class="actions">
  <%= f.submit %>
</div>  

<% end %>




    </div>
  </section>
</div>